@model SistemaConcursoA.Models.RangoEvaluacion
@{
    ViewBag.Title = "Ver";
}

    <h2>Ver</h2>
    <ol class="breadcrumb">
        <li><a href="~/Backend/RangoEvaluacion/Index">Rango Evaluacion</a></li>
        <li class="active">@Model.nombre</li>
    </ol>

    <div class="panel panel-default">
        <div class="panel-heading">Información detallada de rango de evaluacion</div>
        <div class="panel-body">
            <dl class="dl-horizontal">
                <dt>ID</dt>
                <dd>@Model.rango_id</dd>
                <dt>Concurso</dt>
                <dd>@Model.Concurso.nombre</dd>
                <dt>Nombre</dt>
                <dd>@Model.nombre</dd>
                <dt>Puntaje</dt>
                <dd>@Model.puntaje</dd>

            </dl>

        </div>
    </div>